{
  "expo": {
    "name": "TrackAmerica",
    "slug": "trackamerica",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "scheme": "trackamerica",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trackamerica.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.trackamerica.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-router"]
  }
}
